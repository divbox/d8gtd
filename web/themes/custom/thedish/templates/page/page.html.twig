<div class="container-fluid">
  <div class="row">
    {% include '@thedish/partials/header.html.twig' %}
  </div>
</div>

<div class="container">

  <div class="row">

    <div class="col-lg-8 main" role="main-content">
      {{ page.content }}
    </div>

    <div class="col-lg-4 sidebar" role="complementary">
      {% if page.sidebar %}
        <aside>
          {{ page.sidebar }}
          {#% include '@thedish/partials/sidebar.html.twig' %#}
        </aside>
      {% endif %}
    </div>

  </div>

</div>

<div class="container-fluid">
  <div class="row">
    {% include '@thedish/partials/footer.html.twig' %}
  </div>
</div>
