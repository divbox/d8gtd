  </div>
</div>

<div class="container">

  <div class="row">
    <div class="col-lg-8 main" role="main-content">
      {{ page.content }}
    </div>
    <div class="col-lg-4 sidebar">
      {% if page.sidebar %}
        <aside class="layout-sidebar" role="complementary">
          {{ page.sidebar }}
        </aside>
      {% endif %}
    </div>
  </div>

</div>

<div class="container-fluid">
  <div class="row">
    {% include '@thedish/partials/footer.html.twig' %}
  </div>
</div>
